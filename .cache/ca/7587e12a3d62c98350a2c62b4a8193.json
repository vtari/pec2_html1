r\n  background-color: #333;\r\n  position: relative;\r\n}\r\n\r\nnav #menu {\r\n  display: none;\r\n}\r\n\r\nnav a {\r\n  color: white;\r\n  padding: 14px 16px;\r\n  text-decoration: none;\r\n  font-size: 23px;\r\n  display: block;\r\n}\r\n\r\n/* Style the hamburger menu */\r\nnav a.icon {\r\n  background: black;\r\n  display: block;\r\n  position: absolute;\r\n  right: 0;\r\n  top: 0;\r\n}\r\n\r\nnav a:hover {\r\n  color: #ccc;\r\n  text-decoration: underline;\r\n  cursor: pointer;\r\n}\r\n\r\n/* Style the active link (or home/logo) */\r\n.active {\r\n  background-color: #800080;\r\n  color: white;\r\n}\r\n\r\n@media (min-width: 768px) {\r\n  /* Menu */\r\n  nav {\r\n    font-size: 16px;\r\n    margin-top: 0;\r\n    position: relative;\r\n    top: 0;\r\n    width: 100%;\r\n  }\r\n\r\n  nav #menu {\r\n    display: flex;\r\n    flex-direction: row;\r\n    flex-wrap: wrap;\r\n    justify-content: center;\r\n  }\r\n\r\n  nav a.icon {\r\n    display: none;\r\n  }\r\n\r\n  nav ul {\r\n    background-color: #333;\r\n    list-style-type: none;\r\n    margin: 0;\r\n    overflow: hidden;\r\n    padding: 0;\r\n  }\r\n\r\n  nav li {\r\n    float: left;\r\n  }\r\n\r\n  nav a {\r\n    color: #fff;\r\n    display: block;\r\n    padding: 10px 12px;\r\n    text-align: center;\r\n    text-decoration: none;\r\n  }\r\n}\r\n\r\n/* Container */\r\n.container {\r\n  width: 100%;\r\n  height: 100%;\r\n  display: flex;\r\n  flex-direction: row;\r\n  flex-wrap: wrap;\r\n  justify-content: space-around;\r\n  margin-top: 20px;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n/* Footer */\r\n.footer {\r\n  display: grid;\r\n  grid-template-columns: 0.5fr 1fr 1fr 1fr 1fr;\r\n  height: auto;\r\n  background-color: #333;\r\n  color: white;\r\n  right: 0;\r\n  bottom: 0;\r\n  left: 0;\r\n  padding: 1rem;\r\n  text-align: center;\r\n}\r\n\r\n.footer .logo {\r\n  grid-column: 1 / 1;\r\n  background-color: transparent;\r\n}\r\n\r\n.footer .text-center {\r\n  grid-column: 2 / span 3;\r\n}\r\n\r\n/** Elipse **/\r\n.footer .inset {\r\n  grid-column: 5 / 5;\r\n  -webkit-clip-path: inset(45% 0% 33% 10% round 10px);\r\n  clip-path: inset(45% 0% 33% 10% round 10px);\r\n  float: right;\r\n}\r\n.footer .ellipse2 {\r\n  grid-column: 5 / 5;\r\n  width: 80%;\r\n  height: 80%;\r\n  -webkit-clip-path: ellipse(50% 65% at 70% 50%);\r\n  clip-path: ellipse(50% 65% at 70% 50%);\r\n}\r\n\r\n/* articles */\r\narticle {\r\n  width: 100%;\r\n  height: 100%;\r\n  box-shadow: 5px 10px 8px #888;\r\n  background-color: #fff;\r\n  padding: 25px;\r\n}\r\n\r\narticle img {\r\n  width: 100%;\r\n}\r\n\r\n.portada {\r\n  width: 60%;\r\n}\r\n\r\n/* Breadcrumbes */\r\n/* Style the list */\r\nul.breadcrumb {\r\n  padding: 10px 16px;\r\n  list-style: none;\r\n  background-color: #eee;\r\n}\r\n\r\n/* Display list items side by side */\r\nul.breadcrumb li {\r\n  display: inline;\r\n  font-size: 18px;\r\n}\r\n\r\n/* Add a slash symbol (/) before/behind each list item */\r\nul.breadcrumb li + li:before {\r\n  padding: 8px;\r\n  color: black;\r\n  content: \"/\\00a0\";\r\n}\r\n\r\n/* Add a color to all links inside the list */\r\nul.breadcrumb li a {\r\n  color: #0275d8;\r\n  text-decoration: none;\r\n}\r\n\r\n/* Add a color on mouse-over */\r\nul.breadcrumb li a:hover {\r\n  color: #01447e;\r\n  text-decoration: underline;\r\n}\r\n/* Encabezados */\r\n\r\nh1 {\r\n  font-size: 20px;\r\n}\r\n/* h2 */\r\nh2 {\r\n  font-size: 20px;\r\n  height: auto;\r\n  padding: 10px;\r\n}\r\n\r\n/* h4 */\r\nh4 {\r\n  font-size: 18px;\r\n  font-weight: bold;\r\n}\r\n\r\n@media (min-width: 992px) {\r\n  /* h1 */\r\n  h1 {\r\n    font-size: 38px;\r\n  }\r\n  /* h2 */\r\n  h2 {\r\n    font-size: 30px;\r\n  }\r\n\r\n  /* h4 */\r\n  h4 {\r\n    font-size: 24px;\r\n  }\r\n}\r\n\r\n/* blockquote */\r\n.blockquote {\r\n  font-size: 1em !important;\r\n  background-color: #ead7fa;\r\n  font-style: italic;\r\n  border-left: 10px solid #333;\r\n}\r\n\r\n/** Thumbnails **/\r\n.thumbnail {\r\n  border: 1px solid #ddd;\r\n  border-radius: 4px;